<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">


    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/px60dp"
        android:background="@drawable/shape_oneiromancy_btn"
        android:layout_width="@dimen/px650dp"
        android:layout_height="@dimen/px110dp">

        <TextView
            android:textColor="@color/color_text_333"
            android:text="TextView"
            android:textSize="@dimen/px34dp"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_title"
            />
    </RelativeLayout>
</LinearLayout>

