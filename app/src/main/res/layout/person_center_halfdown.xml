<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_mine_sqds"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px100dp"

        android:background="@drawable/selector_touch">

        <ImageView
            android:id="@+id/iv_mine_sqds"
            android:layout_width="@dimen/px40dp"
            android:layout_height="@dimen/px40dp"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/px34dp"
            android:src="@drawable/me_shengqing" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/px30dp"
            android:layout_toRightOf="@+id/iv_mine_sqds"
            android:text="@string/txt_shenqing_dashi"
            android:textColor="@color/mine_txt"
            android:textSize="@dimen/sp_15" />

        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/margin_middle"
            android:src="@drawable/icon_toright" />
    </RelativeLayout>

    <View style="@style/horizontalDivider" />

    <RelativeLayout
        android:id="@+id/rl_mine_dingdan"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px100dp"
